{
  "version": 2,
  "name": "battlesound-client",
  "alias": "battlesound",
  "routes": [
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "^/img/(.*)",
        "dest": "/img/$1"
      },
      {
        "src": ".*",
        "dest": "/index.html"
      }
  ],
  "env": {
    "REACT_APP_API_ENDPOINT": "https://battlesound-server.herokuapp.com/api",
    "REACT_APP_TOKEN_KEY": "bsclienttoken"
  }
}
